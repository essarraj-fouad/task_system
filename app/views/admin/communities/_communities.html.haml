%table#communities.table.table-striped
  %tr
    %th= t('activerecord.attributes.community.name')
    %th= t('activerecord.attributes.community.subdomain')
    %th= t('activerecord.attributes.community.members')
    %th
  = render partial: 'community', collection: @communities
= will_paginate @communities, class: 'pagination pagination-centered pagination-mini'